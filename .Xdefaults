! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #2f343f, #2f343f, #2f343f
rofi.color-normal: #2f343f, #ffffff, #2f343f, #424959, #ffffff
rofi.color-active: #5294e2, #ffffff, #5294e2, #5ca6ff, #ffffff
rofi.color-urgent: #e53935, #ffffff, #e53935, #ff3f3b, #ffffff

Xcursor.theme: Adwaita

urxvt color scheme:

URxvt.perl-ext-common:        default,xkr-clipboard
URxvt.iso14755:               false
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Control-Insert:  perl:clipboard:copy

URxvt*buffered: false
URxvt*background: #2F343F
URxvt*foreground: #DEDEDE

URxvt*colorUL: #86a2b0

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #e25252
URxvt*color9  : #e25252
! green
URxvt*color2  : #8AE234
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #5294e2
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
URxvt*color6  : #52e2d6
URxvt*color14 : #52e2d6
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

!urxvt fonts: 

!URxvt*font: xft:ProggyCleanTT:pixelsize=16
!URxvt*boldFont: xft:ProggyCleanTT:pixelsize=16
URxvt*font: xft:Inconsolata for Powerline:pixelsize=14
URxvt*boldFont: xft:Inconsolata for Powerline:bold:pixelsize=14

!urxvt extensions:

URxvt.perl-ext-common:  default,matcher,tabbedex,clipboard

!enabling clickable links:

URxvt.urlLauncher:      /usr/bin/firefox
URxvt.matcher.button:   1 

!urxvt scrolling options and cursor style:

URxvt*saveLines: 12000
URxvt*scrollstyle:plain
URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt*cursorUnderline: false

!URxvt*transparent: true
URxvt*shading: 20
